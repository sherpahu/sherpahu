关于na,nb变化的情况不适用,待数组学完再来
#include <stdio.h>
int main()
{
int n,na,nb,a1,a2,a3,b1,b2,b3,b4,a,b,score,sum,N;
scanf("%d%d%d\n%d%d%d\n%d%d%d%d",&n,&na,&nb,&a1,&a2,&a3,&b1,&b2,&b3,&b4);
for(N=1;N<=n;N++)
{
    switch(n%3)
    {
    case 1:
    a=a1;
    break;
    case 2:
    a=a2;
    break;
    case 0:
    a=a3;
    break;
    }
    switch(n%4)
    {
    case 1:
    b=b1;
    break;
    case 2:
    b=b2;
    break;
    case 3:
    b=b3;
    break;
    default:
    b=b4;
    break;
    }


if(b=0,a=5)
{
    score=1;
}
else if(a=0,b=5)
{
    score=-1;
}
else
    if(a>b)    
    {
    score=-1;
    }
    if(a<b)
    {
    score=1;
    }
    if(a=b)
    {
    score=0;
    }
sum=score+sum;
}
if(sum>0)
{
printf("A");
}
else if(sum<0)
{
printf("B");
}
else
{
printf("draw");
}

return 0;
}
