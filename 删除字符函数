void delChar(char str[],char c)
{
    int i=0,j;
    while(str[i])//让str[i]进入循环,s[i]='\0'的时候也进入
    {
        while(str[i]!=c&&str[i])//寻找等于c的项
          i++;
        if(s[i]!='\0')
        {
          j=i+1;
          while(s[j])
          {
              s[j-1]=s[j];//移位,让等于c的那一项变成后面的
              j++;
          }
          s[j-1]='\0';
        }
    }
}
