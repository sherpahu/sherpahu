未完待续
#include <stdio.h>
int main()
{
    int n,x,i,a[1000],j,count=1,c=0,max;
    scanf("%d%d",&n,&x);
    for(i=1;i<=n;i++)
    {
        j=i;
        
        while(j!=0)
        {
            a[count++]=j%10;
            j/=10;
        }
        
        
    }
    count=max;
    for(count;count<=max;count++)
    {
        if(a[count]==x)
        {
            c++;
        }
    }
    printf("%d\n",c);
    
    return 0;
}
P1047 校门外的树
#include <stdio.h>
int main()
{
	int l,m,i,j,a[10000],b[10000],zong[100000],sum=0;
	
	
	scanf("%d%d",&l,&m);
	for(i=0;i<=l;i++)
	{
		zong[i]=1;
	}
	for(i=0;i<m;i++)
	{
	scanf("%d%d",&a[i],&b[i]);
		
		for(j=a[i];j<=b[i];j++)
		{
			zong[j]=0;
		}
	
		
	}	
	for(i=0;i<=l;i++)
	{
		sum+=zong[i];
	}
	printf("%d",sum);
	
	return 0;
 } 
