未完待续
#include <stdio.h>
int main()
{
    int n,x,i,a[1000],j,count=1,c=0,max;
    scanf("%d%d",&n,&x);
    for(i=1;i<=n;i++)
    {
        j=i;
        
        while(j!=0)
        {
            a[count++]=j%10;
            j/=10;
        }
        
        
    }
    count=max;
    for(count;count<=max;count++)
    {
        if(a[count]==x)
        {
            c++;
        }
    }
    printf("%d\n",c);
    
    return 0;
}
